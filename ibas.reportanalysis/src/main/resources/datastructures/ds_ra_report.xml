    <Property Name="UpdateActionId" Description="更新动作标识" DataType="Alphanumeric" DataSubType="Default" EditSize="36" Mapped="UpdateActId"/>
    <Property Name="Name" Description="参数名称" DataType="Alphanumeric" DataSubType="Default" EditSize="50" Mapped="Name"/>
    <Property Name="Category" Description="参数类型" DataType="Alphanumeric" DataSubType="Default" EditSize="5" DeclaredType="emReportParameterType" Mapped="Category"/>
    <Property Name="Description" Description="参数说明" DataType="Alphanumeric" DataSubType="Default" EditSize="50" Mapped="Description"/>
    <Property Name="Value" Description="参数值" DataType="Alphanumeric" DataSubType="Default" EditSize="200" Mapped="Value"/>
