<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE configuration>
<ns2:configuration xmlns:ns2="http://colorcoding.org/ibas/bobas/configuration">
    <appSettings>
        <!-- 配置项目-公司标记 -->
        <add key="Company" value="CC" />
        <!-- 配置项目-是否开启调试模式 -->
        <add key="DebugMode" value="true" />
        <!-- 配置项目-数据库类型 -->
        <add key="MasterDbType" value="MSSQL" />
        <!-- 配置项目-数据库地址-->
        <add key="MasterDbServer" value="localhost" />
        <!-- 配置项目-数据库名称 -->
        <add key="MasterDbName" value="ibas_demo" />
        <!-- 配置项目-数据库用户名称 -->
        <add key="MasterDbUserID" value="sa" />
        <!-- 配置项目-数据库用户密码 -->
        <add key="MasterDbUserPassword" value="1q2w3e" />
        <!-- 配置项目-序列化管理方式 -->
        <add key="SerializationWay" value="jersey" />
        <!-- 配置项目-审批方法 -->
        <add key="ApprovalWay" value="fantasy" />
        <!-- 配置项目-组织方式 -->
        <add key="OrganizationWay" value="fantasy" />
        <!-- 配置项目-权限判断方式 -->
        <add key="OwnershipWay" value="fantasy" />
        <!-- 配置项目-报表工厂，多个时“;”分隔 -->
        <add key="ReporterFactories" value="org.colorcoding.ibas.boe.reporter.ReporterFactory;" />
    </appSettings>
</ns2:configuration>
